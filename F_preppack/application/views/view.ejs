<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <link rel="stylesheet" href="/public/css/style.css" type="text/css">
    <meta charset="utf-8">
    <title></title>

  </head>
  <style media="screen">
    .Stuffs{
      padding: 5px;
    }
  </style>
  <body>
    <%console.log(orderlist);%>
    <% if (orderlist) { %>
      <% if (order) { %>
        <p1><%-JSON.stringify(orderlist[order])%></p1>
        <% } else { %>
      <ul>
        <% for (var i in orderlist) { %>
          <div class="Stuffs">
            <p1> Order Number: <%-JSON.stringify(orderlist[i]["orderNumber"])%> </p1>
            <br>
            <p1> Customer Name: <%-JSON.stringify(orderlist[i]["customerName"])%> </p1>
          </div>
          <%  } %>
        </ul>
        <% } %>
    <% } else { %>
      <p1>No orders</p1>
    <% } %>
  </body>
  <div class="outer">
    <div class="inner">
      <button type="button" name="return" id="button"><a href="/" id="button">Return</a></button>

    </div>

  </div>
</html>
